<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.sebastian.reservalabapp.MainActivity">


    <fragment
        android:id="@+id/data_fragment"
        android:name="com.example.sebastian.reservalabapp.fragments.testFragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        tools:layout="@layout/fragment_test" />

    <fragment
        android:name="com.example.sebastian.reservalabapp.fragments.DetailFragment"
        android:id="@+id/data_detail_f"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        tools:layout="@layout/fragment_detail"
        />
